<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /
RewriteCond %{HTTPS} =on
RewriteCond %{HTTP_HOST} ^divi.wojciechsmolarek.pl
# Start WPFC Exclude
# End WPFC Exclude
# Start_WPFC_Exclude_Admin_Cookie
RewriteCond %{HTTP:Cookie} !wordpress_logged_in_[^\=]+\=wojciech
# End_WPFC_Exclude_Admin_Cookie
RewriteCond %{HTTP_HOST} ^divi.wojciechsmolarek.pl
